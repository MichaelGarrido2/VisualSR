<Window x:Class="demo.VariablesList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demo"
        xmlns:core="clr-namespace:VisualSR.Core;assembly=VisualSR"
        xmlns:controls="clr-namespace:VisualSR.Controls;assembly=VisualSR"

        WindowState="Maximized"
        mc:Ignorable="d"
        Title="VariablesList" Height="300" Width="300">
    <Grid Background="#353535">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <core:VirtualControl x:Name="MagicBox" Grid.Column="1" />

        <controls:VariablesList x:Name="vars" Grid.Column="0" />
    </Grid>
</Window>